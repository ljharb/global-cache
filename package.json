		"is-symbol": "^1.0.4"
	},
	"devDependencies": {
		"@ljharb/eslint-config": "^21.0.0",
		"aud": "^2.0.1",
		"eslint": "=8.8.0",
		"nyc": "^10.3.2",
		"rimraf": "^2.7.1",
		"safe-publish-latest": "^2.0.0",
